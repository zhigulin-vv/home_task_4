<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.home_task_4.entities.EmailsEntity" table="emails" schema="db1">
        <property name="email">
            <column name="email" sql-type="char(100)" length="100" not-null="true"/>
        </property>
        <property name="id">
            <column name="id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>